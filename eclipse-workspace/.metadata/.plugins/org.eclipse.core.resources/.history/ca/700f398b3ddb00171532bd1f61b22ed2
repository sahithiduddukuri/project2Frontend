package hello;
public class NumberPattern1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int count=5;
		
		for (int i = 1; i <= count; i++)
		{
			for(int j = 1; j <= i; j++)
			{
				System.out.println(i);
			}
			System.out.println();
		}
	}

}


