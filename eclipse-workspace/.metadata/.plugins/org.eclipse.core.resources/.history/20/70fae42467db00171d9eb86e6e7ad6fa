package hello;

public class NumberPattern2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i = 5;
        while (i >= 1) {
            int j = 5;
            while (j >= i) {
                System.out.print(j);
                j--;
            }
            i--;
            System.out.println();
        }
		       
	}

}
