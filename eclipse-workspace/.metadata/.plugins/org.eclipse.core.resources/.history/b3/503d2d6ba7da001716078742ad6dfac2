package hello;

public class StringOperation {

	public static void main(String[] args) {
		String str="sahi";
		int str1ref=str.hashCode();
		System.out.println(str +"str1ref:"+str1ref);
		int str2ref=str.hashCode();
		System.out.println(str +"str2ref:"+str2ref);
		
		
